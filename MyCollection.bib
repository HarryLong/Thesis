pages = {83--90},
title = {{Harold: A World Made of Drawings}},
year = {2000}
}
@article{Connell2002,
abstract = {Writer-adaptation is the process of converting a$\backslash$nwriter-independent handwriting recognition system into a$\backslash$nwriter-dependent system. It can greatly increasing recognition accuracy,$\backslash$ngiven adequate writer models. The limited amount of data a writer$\backslash$nprovides during training constrains the models' complexity. We show how$\backslash$nappropriate use of writer-independent models is important for the$\backslash$nadaptation. Our approach uses writer-independent writing style models$\backslash$n(lexemes) to identify the styles present in a particular writer's$\backslash$ntraining data. These models are then updated using the writer's data.$\backslash$nLexemes in the writer's data for which an inadequate number of training$\backslash$nexamples is available are replaced with the writer-independent models.$\backslash$nWe demonstrate the feasibility of this approach on both isolated$\backslash$nhandwritten character recognition and unconstrained word recognition$\backslash$ntasks. Our results show an average reduction in error rate of 16.3$\backslash$npercent for lowercase characters as compared against representing each$\backslash$nof the writer's character classes with a single model. In addition, an$\backslash$naverage error rate reduction of 9.2 percent is shown on handwritten$\backslash$nwords using only a small amount of data for adaptation},
author = {Connell, Scott D. and Jain, Anil K.},
doi = {10.1109/34.990135},
file = {:home/harry/Uni/Research/Papers/Clustering/00990135.pdf:pdf},
isbn = {0162-8828},
issn = {01628828},
journal = {IEEE Transactions on Pattern Analysis and Machine Intelligence},
keywords = {Hidden Markov models,Lexemes,Online handwriting,Writer-adaptation,Writing style modeling},
number = {3},
pages = {329--346},
title = {{Writer adaptation for online handwriting recognition}},
volume = {24},
year = {2002}
}
