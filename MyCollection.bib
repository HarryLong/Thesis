@article{Cohen2000,
abstract = {The problem of interactively creating 3D scenes from 2Dinput is a compelling one, and recent progress has been exciting.We present our system,Harold,which combines ideas fromexisting techniques and introduces new concepts to make an interactive systemfor cre- ating 3D worlds. The interface paradigm in Harold is drawing:all objects are created simply by drawing them with a 2D input de- vice. Most of the 3D objects in Harold are collections of planar strokes that are reoriented in a view-dependent way as the cam- era moves through the world. Virtual worlds created in Harold are rendered with a stroke-based system so that a world will maintain a hand-drawn appearance as the user navigates through it. Harold is not suitable for representing certain classes of 3D objects, espe- cially geometrically regular or extremely asymmetric objects.How- ever,Harold supports a large enough class of objects that a user can rapidly create expressive and visually rich 3D worlds.},
author = {Cohen, Jonathan M and Hughes, John F and Zeleznik, Robert C},
file = {:home/harry/Uni/Research/Papers/to{\_}read/A World Made of Drawings.pdf:pdf},
isbn = {1581132778},
