number = {1},
pages = {9--19},
title = {{D{\'{e}}formation asym{\'{e}}trique de houppiers pour la g{\'{e}}n{\'{e}}ration de repr{\'{e}}sentations paysageres r{\'{e}}alistes}},
url = {http://www.irit.fr/REFIG/index.php/refig/article/viewArticle/11},
volume = {1},
