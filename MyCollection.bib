author = {Boudon, F and Mogu{\'{e}}dec, G Le},
file = {:home/harry/Uni/Research/Papers/Ecosystem/D{\'{e}}formation asym{\'{e}}trique de houppiers pour la g{\'{e}}n{\'{e}}ration de repr{\'{e}}sentations paysag{\`{e}}res r{\'{e}}alistes.pdf:pdf},
